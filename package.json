{
  "name": "bw-ae-db",
  "version": "1.0.0",
  "description": "root folder for bw-ae-db",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "install-root": "npm install",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && npm install",
    "install": "concurrently \"npm run install-root\" \"npm run install-frontend\" \"npm run install-backend\"",
    "build-frontend": "cd frontend && npm run build",
    "build-backend": "cd backend && npm run build",
    "build": "concurrently  \"npm run build-frontend\" \"npm run build-backend\" ",
    "start-frontend": "cd frontend && npm run dev",
    "start-backend": "cd backend && npm run dev",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start:prod": "cd backend && npm start",
    "build:prod": "npm run install-root && npm run install-frontend && npm run install-backend && npm run build-frontend && npm run build-backend",
    "go": "git status && git add . && git add . && git status"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "vite": "^4.5.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "typescript": "^5.4.5"
  }
}
